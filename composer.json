{
  "name": "b3n/php-azure-search",
  "description": "A simple PHP Class to communicate with the Microsoft Azure Search REST API",
  "minimum-stability": "stable",
  "license": "MIT",
  "config": {
    "platform": {
      "php": "7.0.32"
    }
  },
  "authors": [
    {
      "name": "Benjamin Hirsch",
      "email": "mail@benjaminhirsch.net"
    }
  ],
  "keywords": [
    "azure",
    "search",
    "microsoft"
  ],
  "require": {
    "php": "^7.0",
    "zendframework/zend-http": "^2.8",
    "ext-json": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^6.0",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "B3N\\Azure\\Search\\": "src/AzureSearch"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "B3NTest\\Azure\\Search\\": "test/"
    }
  }
}
